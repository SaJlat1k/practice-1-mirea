package pract1;

public class Task5 {
    public static void main(String[] args) {
        System.out.println("Аргументы командной строки:");
        for (int i = 0; i < args.length; i++) {
            System.out.println("args[" + i + "] = " + args[i]);
        }
        if (args.length == 0) {
            System.out.println("(нет аргументов)");
        }
    }
}
