package pract1;

public class Task6 {
    public static void main(String[] args) {
        System.out.println("Первые 10 членов гармонического ряда:");
        for (int i = 1; i <= 10; i++) {
            double term = 1.0 / i;
            System.out.printf("1/%d = %.6f%n", i, term);
        }
    }
}
